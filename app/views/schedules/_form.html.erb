<%= form_for @schedule do |f| %>
	<div>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</div>
	<div><%= f.text_field :creator_id, style: "#{'display:none'}" %>
		<%= f.text_field :creator, style: "#{'display:none'}" %>
	</div>
	<div>
		<%= f.label :public_view %>
		<%= f.text_field :public_view %>
	</div>
	<div>
		<%= f.label :private_view %>
		<%= f.text_field :private_view %>
	</div>
	<%= f.submit 'Submit' %>
<% end %>