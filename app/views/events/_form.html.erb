<div class="new-form">
	<%= form_for @event do |f| %>
		<div>
			<%= f.label :name %>
			<%= f.text_field :name %>
		</div>
		<div>
			<%= f.label :creator %>
			<%= f.text_field :creator %>
		</div>
		<div>
			<%= f.label :number_in_attendance %>
			<%= f.text_field :number_in_attendance %>
		</div>
		<div>
			<%= f.label :number_males_attending %>
			<%= f.text_field :number_males_attending %>
		</div>
		<div>
			<%= f.label :number_females_attending %>
			<%= f.text_field :number_females_attending %>
		</div>
		<div>
			<%= f.label :price %>
			<%= f.text_field :price %>
		</div>
		<div>
			<%= f.label :venue_name %>
			<%= f.text_field :venue_name %>
		</div>
		<div>
			<%= f.label :duration %>
			<%= f.text_field :duration %>
		</div>
		<div>
			<%= f.label :calendar_date %>
			<%= f.text_field :calendar_date %>
		</div>
		<div>
			<%= f.label :street_address %>
			<%= f.text_field :street_address %>
		</div>
		<div>
			<%= f.label :city %>
			<%= f.text_field :city %>
		</div>
		<div>
			<%= f.label :state %>
			<%= f.text_field :state %>
		</div>
		<div>
			<%= f.label :zip_code %>
			<%= f.text_field :zip_code %>
		</div>
		<div>
			<%= f.label :host_name %>
			<%= f.text_field :host_name %>
		</div>
		<div>
			<%= f.label :event_type %>
			<%= f.text_field :event_type %>
		</div>
		<%= f.submit 'Submit' %>
	<% end %>
</div>